    "require": {
        "bolt/bolt": ">=2.0.0,<3.0.0",
        "google/apiclient": "1.*",
        "asimlqt/php-google-spreadsheet-client": "2.3.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Anton",
            "email": "steve@stevefanton.com"
        }
